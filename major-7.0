            color: #333;
            margin-bottom: 20px;
            position: relative;
            display: inline-block;
            font-family: "Luckiest Guy", cursive;
            
        }

        .services-section .section-title::after {
            content: '';
            position: absolute;
            left: 50%;
            bottom: -10px;
            width: 60px;
            height: 4px;
            background-color: #ff6600;
            transform: translateX(-50%);
            transition: width 0.3s ease;
        }

        .services-section .section-title:hover::after {
            width: 170px;
        }

        .services-section .section-subtitle {
            font-size: 1.2rem;
            color: #666;
            margin-bottom: 40px;
            font-family: "Luckiest Guy", cursive;
        }

        .card-container {
            display: flex;
            flex-wrap: wrap;
            gap: 20px;
            justify-content: center;
            width: 100%;
            padding: 20px;
        }

        .card {
            position: relative;
            width: 100%;
            height: auto;
            background: white;
            border-radius: 10px;
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
            padding: 20px 30px;
            display: flex;
            flex-direction: column;
            align-items: flex-start;
            justify-content: flex-start;
            font-size: 18px;
            font-weight: bold;
        }
        .card h3 {
            font-size: 20px;
            color: #333;
            margin-bottom: 10px;
        }

        .card p {
            font-size: 16px;
            color: #666;
            margin-bottom: 15px;
            text-align: left;
        }

        .info-box {
            position: absolute;
            background: #007bff;
            color: white;
            padding: 8px 12px;
            font-size: 14px;
            font-weight: bold;
            border-radius: 5px;
        }

        .start-year {
            top: -10px;
            left: -10px;
        }

        .end-year {
            top: 30px;
            left: -10px;
        }

        .average {
            bottom: -10px;
            right: -10px;
            background: #28a745;
        }

        .card .icon {
            font-size: 40px;
            margin-bottom: 15px;
            color: #007bff;
            margin-left: 20px;
        }

        .card .statistics {
            display: flex;
            justify-content: space-between;
            width: 100%;
            font-size: 14px;
            color: #333;
        }

        .card .statistics span {
            font-weight: bold;
        }

        .button-container {
            display: flex;
            justify-content: center;
            margin-top: 20px;
            padding: 20px;
        }

        .download-btn {
            padding: 12px 20px;
            background-color: #007bff;
            color: white;
            border: none;
            border-radius: 5px;
            font-size: 16px;
            cursor: pointer;
            text-align: center;
            text-decoration: none;
        }

        .download-btn:hover {
            background-color: #0056b3;
        }

        /* Skills Section */
        .skills-container {
            display: flex;
            flex-wrap: wrap;
            gap: 30px;
            justify-content: center;
            margin-top: 50px;
            width: 100%;
            padding: 20px;
        }

        .skill-card {
            background: #fff;
            border-radius: 10px;
            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
            width: 280px;
            padding: 20px;
            text-align: center;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
        }

        .skill-card h4 {
            color: #333;
            font-size: 22px;
            margin-bottom: 15px;
            font-family: "DM Sans", sans-serif;

        }

        .skill-icon {
    position: relative;
    width: 120px; /* Same size as progress-ring */
    height: 120px; /* Same size as progress-ring */
    border-radius: 50%;
    background: #007bff; /* Background color for the icon container */
    display: flex;
    justify-content: center;
    align-items: center;
    color: white; /* Icon color */
    font-size: 48px; /* Adjust icon size */
    margin-bottom: 15px; /* Same spacing as progress-ring */
    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1); /* Optional: Add shadow */
}

.skill-icon i {
    transition: transform 0.3s ease; /* Add hover effect */
}

.skill-icon:hover i {
    transform: scale(1.1); /* Slightly enlarge icon on hover */
}

        .skill-description {
            color: #666;
            font-size: 14px;
            margin-top: 10px;
            font-family: "Anton", sans-serif;
        }

        
        /* Responsive Adjustments */
        @media (max-width: 768px) {
            .card {
                width: 90%;
            }

            .skill-card {
                width: 90%;
            }
        }

        @media (min-width: 1024px) {
            .card-container {
                justify-content: space-around;
            }

            .card {
                width: 45%;
                max-width: 600px;
            }
        }

         /* Responsive Adjustments */

        .footer-section {
    background-color: #1f2937;
    color: #fff;
    padding: 3rem 0;
}

.footer-section h3 {
    font-size: 1.75rem;
    margin-bottom: 1rem;
    font-weight: 700;
    text-align: left;
}

.footer-section ul {
    list-style: none;
    padding: 0;
    margin: 0;
    text-align: left;
}

.footer-section li {
    display: flex;
    align-items: center;
    gap: 0.75rem;
    margin-bottom: 0.75rem;
}

.footer-section a {
    color: #9ca3af;
    text-decoration: none;
    transition: color 0.3s ease;
    font-size: 1.1rem;
}

.footer-section a:hover {
    color: #fff;
}

.footer-section .text-center {
    margin-top: 2rem;
    font-size: 0.875rem;
    color: #6b7280;
    text-align: center;
}

.footer-section .text-center:hover {
    color: #fff;
}

.footer-section .container {
    display: grid;
    grid-template-columns: repeat(3, 1fr); /* Equal-width columns */
    gap: 2rem; /* Space between columns */
    padding: 0 2rem; /* Equal padding on left and right */
    max-width: 1200px; /* Optional: Limit container width */
    margin: 0 auto; /* Center the container */
}

.footer-section ul li i {
    margin-right: 0.5rem;
}

/* Responsive Design */
@media (max-width: 768px) {
    .footer-section h3 {
        font-size: 1.5rem;
    }
    .footer-section a {
        font-size: 1rem;
    }
    .footer-section .container {
        grid-template-columns: 1fr; /* Stack columns on mobile */
        padding: 0 1rem; /* Adjust padding for mobile */
    }
}


    .logos-slider {
        width: 100%;
        overflow: hidden;
        position: relative;
        margin-top: 2rem; /* Add some space between title and logos */
    }

    .logos-track {
        display: flex;
        animation: slide 20s linear infinite;
    }

    .logos-track:hover {
        animation-play-state: paused; /* Pause on hover */
    }

    .logo-item {
        flex: 0 0 auto;
        margin: 0 2rem;
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .logo-item img {
        max-width: 100px;
        height: auto;
        filter: grayscale(100%);
        transition: filter 0.3s ease, transform 0.3s ease;
    }

    .logo-item img:hover {
        filter: grayscale(0%); /* Add color on hover */
        transform: rotate(360deg); /* Rotate on hover */
    }

    /* Animation for sliding */
    @keyframes slide {
        0% {
            transform: translateX(0);
        }
        100% {
            transform: translateX(-50%); /* Move halfway for infinite effect */
        }
    }







         
