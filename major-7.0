        /* Reset and General Styles */
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: Arial, sans-serif;
            scroll-behavior: smooth;
        }

        body {
            background-color: #f5f5f5;
            color: #333;
        }

        /* Navbar */
        .navbar {
            display: flex;
            justify-content: space-between;
            align-items: center;
            background: #222;
            color: white;
            padding: 15px 5%;
            position: sticky;
            top: 0;
            z-index: 1000;
        }

        .navbar .logo {
            font-size: 1.5rem;
            font-weight: bold;
            font-family: "Luckiest Guy", cursive;
        }

        .navbar .nav-menu {
            display: flex;
            align-items: center;
            gap: 20px;
        }

        .navbar ul {
            list-style: none;
            display: flex;
            gap: 15px;
        }

        .navbar ul li a {
            text-decoration: none;
            color: white;
            font-weight: bold;
            transition: color 0.3s ease;
            display: flex;
            align-items: center;
            gap: 8px;
            font-family: "Luckiest Guy", cursive;
        }

        .navbar ul li a:hover {
            color: #ff6600;
        }

        .navbar .download-btn {
            padding: 10px 20px;
            background-color: #ff6600;
            color: white;
            border: none;
            border-radius: 5px;
            font-size: 1rem;
            cursor: pointer;
            transition: background 0.3s ease;
            display: flex;
            align-items: center;
            gap: 8px;
        }

        .navbar .download-btn:hover {
            background-color: #cc5500;
        }

        /* Hamburger Menu */
        .navbar .hamburger {
            display: none;
            font-size: 1.5rem;
            cursor: pointer;
        }

        /* Responsive Adjustments */
        @media (max-width: 768px) {
            .navbar .nav-menu {
                display: none;
                flex-direction: column;
                position: absolute;
                top: 100%;
                left: 0;
                width: 100%;
                background: #222;
                padding: 10px 0;
                gap: 10px;
            }

            .navbar .nav-menu.active {
                display: flex;
            }

            .navbar .hamburger {
                display: block;
            }

            .navbar ul {
                flex-direction: column;
                gap: 10px;
            }

            .navbar .download-btn {
                width: 100%;
                justify-content: center;
            }
        }

        /* Home Page */
        .home-page {
            min-height: 100vh;
            display: flex;
            justify-content: space-around;
            align-items: center;
            padding: 50px 5%;
            background-color: #fff;
            flex-wrap: wrap;
            position: relative;
        }

        .home-page-text {
            flex: 1;
            max-width: 50%;
            position: relative;
            z-index: 1;
            padding: 20px;
            border-radius: 15px;
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
            background-color: #cc5500;  /* background: rgba(255, 255, 255, 0.8); */
        }

        .home-page-text::before {
            content: '';
            position: absolute;
            top: -20px;
            left: -20px;
            right: -20px;
            bottom: -20px;
            background: linear-gradient(135deg, #007bff, #00bfff);
            z-index: -1;
            border-radius: 25px;
            animation: pulseBackground 10s ease-in-out infinite;  /* Twick a bit */
            width: 40%;
        }

    
        @keyframes pulseBackground {
            0% {
                transform: scale(1);
            }
            50% {
                transform: scale(1.05);
            }
            100% {
                transform: scale(1);
            }
        }



        .home-page-text h4 {
            font-size: 2.5rem;
            margin-bottom: 20px;
            color: whitesmoke;
            font-family: "Luckiest Guy", cursive;
        }

        .home-page-text p {
            font-size: 1.2rem;
            color: black;
            margin-bottom: 10px;
            font-family: "Luckiest Guy", cursive;
        }

        .home-page-text .button-container {
            display: flex;
            align-items: center;
            gap: 10px;
            position: relative;
            z-index: 1;
        }

        .button-container::before {
            content: '';
            position: absolute;
            background: linear-gradient(135deg, #ff6600, #ffcc00);
            padding: 40px 40px;
            width: 490px;
            z-index: -1;
            border-radius: 15px;
            animation: pulseBackground 2s ease-in-out infinite;
        }

        @keyframes pulseBackground {
            0% {
                transform: scale(1);
            }
            50% {
                transform: scale(1.05);
            }
            100% {
                transform: scale(1);
            }
        }

        .home-page-text .button-container button {
            background-color: #cc5500;
            padding: 14px 47px;
            border-radius: 5px;
            border: none;
            color: #f5f5f5;
            cursor: pointer;
        }

        .home-page-text .button-container button:hover {
            background-color: #b54400;
        }

        .home-page-text .button-container .chatbot-btn {
            width: 50px;
            height: 50px;
            background-color: #007bff;
            border-radius: 50%;
            display: flex;
            justify-content: center;
            align-items: center;
            color: white;
            font-size: 1.5rem;
            cursor: pointer;
            transition: background-color 0.3s ease;
            position: relative;
            z-index: 1;
        }

        .chatbot-btn::before {
            content: '';
            position: absolute;
            top: -5px;
            left: -5px;
            right: -5px;
            bottom: -5px;
            background: linear-gradient(135deg, #007bff, #00bfff);
            z-index: -1;
            border-radius: 50%;
            animation: rotateBackground 5s linear infinite;
        }

        .home-page-text .button-container .chatbot-btn:hover {
            background-color: #0056b3;
        }

        /* Image Container */
        .image-container {
            position: relative;
            width: 400px;
            height: 400px;
            background-color: #cc5500;
            border-radius: 50%;
            display: flex;
            justify-content: center;
            align-items: center;
            overflow: visible;
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);
            animation: pulseBackground 2s ease-in-out infinite;
        }

        .illustration {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            width: 160px;
            height: 160px;
            background-color: #fff;
            border-radius: 50%;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 2.5rem;
            color: #cc5500;
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);
        }

        .left-shapes {
            position: absolute;
            left: -120px;
            top: 50%;
            transform: translateY(-50%);
            display: flex;
            flex-direction: column;
            gap: 30px;
            z-index: 2;
        }

        .left-shapes .shape {
            width: 14em;
            height: 3em;
            background-color: aqua;
            border-radius: 5px;
            display: flex;
            align-items: center;
            text-align: left;
            font-size: 1rem;
            color: black;
            font-weight: bold;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
            padding: 0 1em;
            box-sizing: border-box;
            gap: 0.5em;
            
            
        }

        .left-shapes .shape i {
            font-size: 1.5em;
            color: white;
        }

        /* Social Icons */
        .social-icons-below {
            display: flex;
            justify-content: center;
            gap: 20px;
            margin-top: 40px;
            width: 100%;
        }

        .social-icons-below a {
            display: flex;
            justify-content: center;
            align-items: center;
            width: 60px;
            height: 60px;
            background-color: #007bff;
            border-radius: 50%;
